@model Senparc.Weixin.MP.AdvancedAPIs.OAuth.OAuthUserInfo
@{
    ViewBag.Title = Model.nickname+ "的信息";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>CallBack</h2>

<h4>从页面："@ViewData["Msg"]" 认证完成后跳转过来的</h4>

<p>昵称：@Model.nickname</p>
<p>性别：@Model.sex</p>
<p>城市：@Model.city</p>
<p>OpenId：@Model.openid</p>
<p><img src="@Model.headimgurl" alt="PhotoImage" style="width: 100px;height:100px"/></p>

